# Mux Backend Environment Variables
# Copy this file to .env and fill in your Mux credentials

# Mux API Credentials (get these from https://dashboard.mux.com/settings/access-tokens)
MUX_TOKEN_ID=your_mux_token_id_here
MUX_TOKEN_SECRET=your_mux_token_secret_here

# Server Settings
PORT=3000
NODE_ENV=development

# Optional: Webhook secret for Mux webhooks
MUX_WEBHOOK_SECRET=your_webhook_secret_here
